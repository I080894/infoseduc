<br/>
 <table  align="center" width="100%">
  <td>
  <fieldset><legend><font  size=5> EQUIPAMENTO CADASTRADOS ADMINISTRAÇÃO</font></legend>
   <br/>
   <div class="botao">
   <table width="100%">
    <tr>
     <% if current_user.has_role?('administrador') or current_user.has_role?('estagiario SEDUC') %>
        <td align="left" width="8%"><%= button_to 'Cadastrar', new_administracao_path %></td>
     <%end%>
     <td align="left" width="8%"><%= button_to 'Voltar', homes_path, {:method => "get"}%></td>
     <td align="left" width="8%"><input type="Button" Value="Imprimir" onclick="javascript:window.print()"></td>
     <% if  $var == 0 then %>
         <td align="left" colspan="4"><%= will_paginate @administracaos %></td>
      <%end%>
      <td align="right">
       <div class="botao">
         <table>
        <tr>
        <% form_tag administracaos_path, :method => 'get' do %>
        <td align="right"><font size="4"> Digite Unidade:</font>
           <%= text_field_tag :search, params[:search] %>
           <%= submit_tag "Filtrar", :nome => nil %>
        </td>
        <% end %>
        </tr>
     </table>
     </div>
     </td>
     </tr>
  </table>
  <br/>
  </div>
   <table align="center" width="100%">

    <tr>
      <td valign="top" width="50%">
        <fieldset><legend><font  size=4> Pertencentes à SEDUC</font></legend>
         <table border="2" width="100%">
          <tr><td>
           <font  size="2">
            <table border="2" width="100%">
              <% for administracaos in @administracaos %>
               <tr>
                <th bgcolor="gray">Unidade</th>
                <th bgcolor="gray">Servidor</th>
                <th bgcolor="gray">Rede</th>
                <th bgcolor="gray">Internet</th>
                <th bgcolor="gray">Obs</th>
               </tr>
               <tr>
                <td align="left"><%=h administracaos.unidade.nome %></td>
                <td align="left"><%=h administracaos.servidor %></td>
                <td align="left"><%=h administracaos.rede %></td>
                <td align="left"><%=h administracaos.internet %></td>
                <td align="left"><%=h administracaos.obs %></td>
                </tr>
              <tr>
                <th rowspan="2" ></th>
                <th bgcolor="gray">Nº Comp</th>
                <th bgcolor="gray">Configuração</th>
                <th bgcolor="gray">Nº imp</th>
                <th bgcolor="gray">Configuração</th>
              </tr>
 
              <tr>

                <td align="center"><%=h administracaos.num_comp_seduc %></td>
                <td align="left"><%=h administracaos.config_comp_seduc %></td>
                <td align="center"><%=h administracaos.num_imp_seduc %></td>
                <td align="left"><%=h administracaos.config_imp_seduc %></td>
              </tr>
              <tr>
                <td colspan="12">
                <div class="botao">
                  <table>
                    <td><%= button_to('Exibir',{:action => "show", :id =>administracaos}, :method => "get") %></td>
                    <% if current_user.has_role?('administrador') or current_user.has_role?('estagiario SEDUC') %>
                       <td> <%= button_to('Editar',{:action => "edit", :id =>administracaos}, :method => "get") %></td>
                       <td><%= button_to('Excluir',{:action => "destroy", :id =>administracaos}, :confirm => 'Confirma exclusão?', :method => "delete")%></td>
                    <%end%>
                  </table>
                </div>
               </td>
            </tr>
              <% end %>
            </table>
           </font>

          </td></tr>
         </table>
        </fieldset>
      </td>
      <td valign="top" width="50%">
        <fieldset><legend><font  size=4> Pertencentes à Terceiros</font></legend>
         <font  size="2">
         <table border="2" width="100%">
         <% for administracaos in @administracaos %>
           <tr>
             <th bgcolor="gray">Unidade</th>
             <th bgcolor="gray">Nº Comp</th>
             <th bgcolor="gray">Configuração</th>
             <th bgcolor="gray">Pertence</th>
             <th bgcolor="gray">Contato</th>
           </tr>
          <tr>
             <td align="left"><%=h administracaos.unidade.nome %></td>
             <td align="center"><%=h administracaos.num_comp %></td>
             <td align="left"><%=h administracaos.config_comp %></td>
             <td align="left" ><%=h administracaos.pertence_comp %></td>
             <td align="left"><%=h administracaos.contato_comp %></td>
           </tr>
          <tr>
             <th rowspan="2" ></th>
             <th bgcolor="gray">Nº Imp</th>
             <th bgcolor="gray">Configuração</th>
             <th bgcolor="gray">Pertence</th>
             <th bgcolor="gray">Contato</th>
           </tr>

           <tr>

             <td align="center"><%=h administracaos.num_imp %></td>
             <td align="left"><%=h administracaos.config_imp %></td>
             <td align="left"><%=h administracaos.pertence_imp %></td>
             <td align="left"><%=h administracaos.contato_imp %></td>
          </tr>
           <tr>
                <td colspan="12">
                <div class="botao">
                  <table>
                    <td><%= button_to('Exibir',{:action => "show", :id =>administracaos}, :method => "get") %></td>
                   <% if current_user.has_role?('administrador') or current_user.has_role?('estagiario SEDUC') %>
                      <td> <%= button_to('Editar',{:action => "edit", :id =>administracaos}, :method => "get") %></td>
                      <td><%= button_to('Excluir',{:action => "destroy", :id =>administracaos}, :confirm => 'Confirma exclusão?', :method => "delete")%></td>
                   <%end%>
                  </table>
                </div>
               </td>
            </tr>
         <% end %>
        </table>
        </font>
        </fieldset>
      </td>
     </table>
   </fieldset>
  </td>
</table>
 <div class="botao">
   <table width="100%">
    <tr>
      <% if current_user.has_role?('administrador') or current_user.has_role?('estagiario SEDUC') %>
         <td width="6%" align="left"><%= button_to 'Cadastrar', new_administracao_path %></td>
      <%end%>
      <td width="6%" align="left"><%= button_to 'Voltar', homes_path, {:method => "get"}%></td>
      <td align="left"><input type="Button" Value="Imprimir" onclick="javascript:window.print()"></td>
      <% if  $var == 0 then %>
         <tr><td align="left" colspan="4"><%= will_paginate @administracaos %></td></tr>
      <%end%>
    </tr>
  </table>
  </div>
