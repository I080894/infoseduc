<br/>
<font  size="4"><b><u> Resultado da Consulta</b</u></font>
  <br/>  <br/>
   <font  size="2">
    <table border="2" width="100%">
      <tr>
        <th bgcolor="gray">Unidade</th>
        <th bgcolor="gray">Nº Comp</th>
        <th bgcolor="gray">Config Comp</th>
        <th bgcolor="gray">Nº Impr</th>
        <th bgcolor="gray">Config Imp</th>
        <th bgcolor="gray">Servidor</th>
        <th bgcolor="gray">rede</th>
        <th bgcolor="gray">Internet</th>
        <th bgcolor="gray">Observações</th>
  </tr>

     <% for laboratorios in @laboratorios %>
     <tr>
        <td><%=h laboratorios.unidade.nome %></td>
        <td><%=h laboratorios.num_comp %></td>
        <td><%=h laboratorios.config_comp %></td>
        <td><%=h laboratorios.num_imp %></td>
        <td><%=h laboratorios.config_imp %></td>
        <td><%=h laboratorios.servidor %></td>
        <td><%=h laboratorios.rede %></td>
        <td><%=h laboratorios.internet %></td>
        <td><%=h laboratorios.obs %></td>
    </tr>
    <tr>
      <td colspan="12">
        <div class="botao">
        <table>
           <td><%= button_to('Exibir',{:action => "show", :id =>laboratorios}, :method => "get") %></td>
           <% if current_user.has_role?('administrador') or current_user.has_role?('estagiario SEDUC') %>
             <td> <%= button_to('Editar',{:action => "edit", :id =>laboratorios}, :method => "get") %></td>
             <td><%= button_to('Excluir',{:action => "destroy", :id =>laboratorios}, :confirm => 'Confirma exclusão?', :method => "delete")%></td>
            <%end%>
        </table>
        </div>
       </td>
    </tr>
    <% end %>
   </table>
   </font>
 




