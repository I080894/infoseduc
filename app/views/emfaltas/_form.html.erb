
  <% form_for(@emfaltas) do |f| %>
  <%= f.error_messages %>
  <font  size="2">
  <table width="100%" align="center">
     <tr>
        <td align="left"><b>Componente:</b></td>
        <td align="left"><%= f.collection_select :componente_id, @componentes, :id, :nome, :include_blank => ' -- Selecione -- ' %>
     </tr>
     <tr>
        <td align="left"><b>Especificação:</b></td>
        <td align="left"><%= f.text_field :especifica %></td>
     </tr>
     <tr>
        <td align="left"><b>Quantidade:</b></td>
        <td align="left"><%= f.text_field :quant, :size => 4 %></td>
     </tr>
     <tr>
         <td valign="top"align="left"><b>Observação:</b></td>
         <td align="left"><%= f.text_area :obs, :cols =>38, :rows => "2" %></td>
     </tr>
  <table>
  <tr>
      <td align="left" width="6%">
        <font size="4" color="red">
        <div id='Certeza'>
          <%= f.submit "Salvar" %>
        </div>
        </font>
      </td>
      <%end%>
     <td align="left" ><%= button_to 'Voltar', homes_path, {:method => "get"}%>
     </td>
  </tr>
</table>
</table>
</font>
